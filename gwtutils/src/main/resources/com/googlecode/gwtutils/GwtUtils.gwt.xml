<?xml version="1.0" encoding="UTF-8"?>
<module>
	<inherits name='com.google.gwt.user.User' />

	<source path='client' />
	
	<replace-with class="com.googlecode.gwtutils.client.console.ChromeConsole">
		<when-type-is class="com.googlecode.gwtutils.client.console.Console" />
		<when-property-is name="user.agent" value="safari" />
	</replace-with>
</module>